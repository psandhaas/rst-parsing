( Root (span 1 20) 
  ( Nucleus (span 1 2) (rel2par span)
    ( Nucleus (leaf 2) (rel2par span) (text _!Kurzarbeit steigt_!) )
      ( Nucleus (leaf 1) (rel2par Contrast) (text _!Arbeitslosenzahlen rückläufig,_!) )
  )
  ( Satellite (span 3 20) (rel2par Elaboration)
    ( Nucleus (span 3 15) (rel2par span)
      ( Nucleus (span 3 10) (rel2par Joint)
        ( Nucleus (leaf 3) (rel2par span) (text _!Die Zahl der Arbeitslosen ist im Vergleich zur Vorwoche leicht gesunken._!) )
        ( Satellite (span 4 10) (rel2par Elaboration)
          ( Nucleus (span 4 5) (rel2par Joint)
            ( Nucleus (leaf 4) (rel2par span) (text _!Derzeit sind 406.987 Personen beim AMS arbeitslos gemeldet oder in Schulung,_!) )
            ( Satellite (leaf 5) (rel2par Elaboration) (text _!das sind um 3.520 Personen weniger als noch vor einer Woche._!) )
          )
          ( Nucleus (span 6 10) (rel2par Joint)
            ( Nucleus (span 6 8) (rel2par Joint)
              ( Nucleus (span 6 7) (rel2par span)
                ( Nucleus (leaf 6) (rel2par Joint) (text _!336.996 Personen sind auf Arbeitssuche,_!) )
                ( Nucleus (leaf 7) (rel2par Joint) (text _!69.991 nehmen an Schulungsmaßnahmen teil,_!) )
              )
              ( Satellite (leaf 8) (rel2par Attribution) (text _!geht aus aktuellen Zahlen des Arbeitsministeriums hervor._!) )
            )
            ( Nucleus (span 9 10) (rel2par Joint)
              ( Nucleus (leaf 9) (rel2par span) (text _!Ein Anstieg wurde bei den Anmeldungen zur Kurzarbeit verzeichnet,_!) )
              ( Satellite (leaf 10) (rel2par Elaboration) (text _!diese kletterten von 114.961 auf 136.995._!) )
            )
          )
        )
      )
      ( Nucleus (span 11 15) (rel2par Joint)
        ( Nucleus (leaf 11) (rel2par Joint) (text _!“Trotz der für diese Zeit des Jahres üblichen saisonalen Effekte verzeichnen wir derzeit sinkende Arbeitslosenzahlen._!) )
        ( Nucleus (span 12 15) (rel2par Joint)
          ( Nucleus (leaf 12) (rel2par span) (text _!Bemerkenswert ist, dass die Arbeitslosigkeit mit derzeit 406.987 arbeitslos gemeldeten Personen inklusive Teilnehmerinnen und Teilnehmern an AMS Schulungen um knapp 23.700 Personen unter dem Niveau der Vergleichswoche 2019 liegt”,_!) )
          ( Satellite (span 13 15) (rel2par Attribution)
            ( Nucleus (span 13 14) (rel2par Same-Unit)
              ( Nucleus (leaf 13) (rel2par span) (text _!erklärte Arbeitsminister Martin Kocher_!) )
              ( Satellite (leaf 14) (rel2par Elaboration) (text _!(ÖVP)_!) )
            )
            ( Nucleus (leaf 15) (rel2par Same-Unit) (text _!am Dienstag laut Aussendung._!) )
          )
        )
      )
    )
    ( Satellite (span 16 20) (rel2par Elaboration)
      ( Nucleus (span 16 17) (rel2par Joint)
        ( Nucleus (leaf 16) (rel2par span) (text _!Die rückläufigen Zahlen zeigen, dass die Auswirkungen der Pandemie den Arbeitsmarkt nur mehr in gewissen Bereichen wie der Langzeitarbeitslosigkeit prägen,_!) )
        ( Satellite (leaf 17) (rel2par Attribution) (text _!hieß es in der Aussendung._!) )
      )
      ( Nucleus (span 18 20) (rel2par Joint)
        ( Nucleus (leaf 18) (rel2par span) (text _!Positiv sei auch, dass sich die Jugendarbeitslosigkeit nach nationaler Definition auf niedrigerem Niveau als vor der Krise bewege._!) )
        ( Satellite (span 19 20) (rel2par Elaboration)
          ( Nucleus (leaf 19) (rel2par Comparison) (text _!Die Quote liegt derzeit bei 6,9 Prozent,_!) )
          ( Nucleus (leaf 20) (rel2par Comparison) (text _!2019 waren es noch 7,6 Prozent._!) )
        )
      )
    )
  )
)

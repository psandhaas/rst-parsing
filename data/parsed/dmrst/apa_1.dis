( Root (span 1 19) 
  ( Satellite (leaf 1) (rel2par Background) (text _!Ministerium bestätigte Omikron-Fall in Österreich_!) )
  ( Nucleus (span 2 19) (rel2par span)
    ( Nucleus (span 2 17) (rel2par Joint)
      ( Nucleus (span 2 14) (rel2par Joint)
        ( Nucleus (span 2 8) (rel2par Joint)
          ( Nucleus (span 2 6) (rel2par span)
            ( Nucleus (span 2 5) (rel2par span)
              ( Nucleus (span 2 4) (rel2par span)
                ( Nucleus (leaf 2) (rel2par span) (text _!Die Coronavirus-Variante Omikron ist offiziell in Österreich angekommen:_!) )
                ( Satellite (span 3 4) (rel2par Elaboration)
                  ( Nucleus (leaf 3) (rel2par span) (text _!“Im Gesundheitsministerium liegen jetzt sämtliche Ergebnisse vor, die es für eine Bestätigung braucht”,_!) )
                  ( Satellite (leaf 4) (rel2par Attribution) (text _!hieß es aus dem Ressort._!) )
                )
              )
              ( Satellite (leaf 5) (rel2par Elaboration) (text _!Bei einem Fall aus Tirol handelt es sich “mit Sicherheit” um die Variante mit der wissenschaftlichen Bezeichnung B.1.1.529._!) )
            )
            ( Satellite (leaf 6) (rel2par Elaboration) (text _!Die Tiroler Behörden hatten den Verdachtsfall am Samstagabend bekanntgegeben._!) )
          )
          ( Satellite (span 7 8) (rel2par Elaboration)
            ( Nucleus (leaf 7) (rel2par Joint) (text _!Von der Infektion betroffen sei eine Person, die nach einer Südafrika-Reise positiv auf Covid-19 getestet wurde und derzeit keine Symptome aufweise._!) )
            ( Nucleus (leaf 8) (rel2par Joint) (text _!Laut Elmar Rizzoli, Leiter des Tiroler Corona-Einsatzstabes, wurden alle Kontaktpersonen umgehend abgesondert._!) )
          )
        )
        ( Nucleus (span 9 14) (rel2par Joint)
          ( Nucleus (span 9 10) (rel2par Joint)
            ( Nucleus (leaf 9) (rel2par Temporal) (text _!Das Land Tirol forderte alle Personen, die in den vergangenen 14 Tagen aus den Ländern Südafrika, Lesotho, Botswana, Simbabwe, Mosambik, Namibia und Eswatini zurückgekehrt sind, auf, einen PCR-Test zu machen._!) )
            ( Nucleus (leaf 10) (rel2par Temporal) (text _!Dies sollte am fünften und zehnten Tag nach der Einreise wiederholt werden._!) )
          )
          ( Nucleus (span 11 14) (rel2par Joint)
            ( Nucleus (leaf 11) (rel2par Joint) (text _!Bisher meldeten sich 31 Menschen._!) )
            ( Nucleus (span 12 14) (rel2par Joint)
              ( Nucleus (span 12 13) (rel2par span)
                ( Nucleus (leaf 12) (rel2par Joint) (text _!“Neben dem genannten einen positiven Ergebnis liegt für 20 Personen bereits ein negatives Testergebnis vor,_!) )
                ( Nucleus (leaf 13) (rel2par Joint) (text _!bei elf Personen sind die Testungen aktuell im Gange”,_!) )
              )
              ( Satellite (leaf 14) (rel2par Attribution) (text _!hieß es._!) )
            )
          )
        )
      )
      ( Nucleus (span 15 17) (rel2par Joint)
        ( Satellite (leaf 15) (rel2par Background) (text _!Die Omikron-Variante von SARS-CoV-2 sorgt weltweit seit Tagen für Schlagzeilen._!) )
        ( Nucleus (span 16 17) (rel2par span)
          ( Nucleus (leaf 16) (rel2par span) (text _!Die WHO arbeitet nach eigenen Angaben mit technischen Partnern zusammen,_!) )
          ( Satellite (leaf 17) (rel2par Enablement) (text _!um die Auswirkungen der Variante auf die bestehenden Gegenmaßnahmen wie Impfstoffe zu bewerten._!) )
        )
      )
    )
    ( Nucleus (span 18 19) (rel2par Joint)
      ( Nucleus (leaf 18) (rel2par Joint) (text _!Es sei noch unklar, ob B.1.1.259 leichter übertragbar verglichen mit anderen Covid-19-Varianten sei_!) )
      ( Nucleus (leaf 19) (rel2par Joint) (text _!oder einen schwereren Krankheitsverlauf nach sich ziehe._!) )
    )
  )
)

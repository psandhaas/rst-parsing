( Root (span 1 13) 
  ( Satellite (leaf 1) (rel2par Background) (text _!HEIKO HOHENHAUS_!) )
  ( Nucleus (span 2 13) (rel2par span)
    ( Satellite (leaf 2) (rel2par Background) (text _!Notsignale_!) )
    ( Nucleus (span 3 13) (rel2par span)
      ( Nucleus (span 3 13) (rel2par span)
        ( Nucleus (span 3 10) (rel2par span)
          ( Nucleus (leaf 3) (rel2par Contrast) (text _!Die Oranienburger Mittelstadt kann sich glücklich schätzen, dass sie zu den 15 ausgewählten Stadtteilen im Land Brandenburg gehört, die in den nächsten fünf Jahren vom 220 Millionen Mark schweren EU-Förderprogramm "Zukunft im Stadtteil" (ZiS) profitieren werden._!) )
          ( Nucleus (leaf 4) (rel2par span) (text _!Eine erste Bestandsaufnahme der Situation in der Mittelstadt brachte nun ein überraschend negatives Bild._!) )
          ( Satellite (span 5 10) (rel2par Elaboration)
            ( Nucleus (span 5 6) (rel2par Contrast)
              ( Nucleus (leaf 6) (rel2par span) (text _!einen Vergleich mit schwer gebeutelten Neubaugebieten, wie etwa Velten-Süd, hätten Kommunalpolitiker aber abgelehnt._!) )
                ( Nucleus (leaf 5) (rel2par Contrast) (text _!Bisher wurde ihr Zustand zwar als nicht unproblematisch eingeschätzt,_!) )
            )
            ( Nucleus (span 7 10) (rel2par Contrast)
              ( Nucleus (leaf 7) (rel2par Joint) (text _!Eine Bürgerbefragung ergab jedoch, dass mehr als die Hälfte der Bewohner der Mittelstadt schon über den Wegzug nachgedacht hat._!) )
              ( Nucleus (span 8 10) (rel2par Joint)
                ( Nucleus (leaf 8) (rel2par Joint) (text _!Eine bedeutende Minderheit schätzt die Wohnqualität als "schlecht" oder "sehr schlecht" ein._!) )
                ( Nucleus (span 9 10) (rel2par Joint)
                  ( Nucleus (leaf 9) (rel2par Joint) (text _!Als deutliches Zeichen für den beginnenen Abstieg wird der Einbau von Sperrgittern an Durchgängen zu Wohnanlagen gewertet._!) )
                  ( Nucleus (leaf 10) (rel2par Joint) (text _!Einwohner sprechen von ständigem Vandalismus im Umfeld der Schulen._!) )
                )
              )
            )
          )
        )
        ( Nucleus (span 11 13) (rel2par Evaluation)
          ( Nucleus (leaf 13) (rel2par span) (text _!Das aber wird ohne ein Mitwirken der Mieter und Vermieter in der Mittelstadt nicht funktionieren._!) )
            ( Nucleus (span 11 12) (rel2par Contrast)
              ( Nucleus (leaf 11) (rel2par Joint) (text _!Dass es nach den Notsignalen wieder Zeichen der Hoffnung gibt,_!) )
              ( Nucleus (leaf 12) (rel2par Joint) (text _!dafür könnte das ZiS-Programm sorgen._!) )
            )
        )
      )
    )
  )

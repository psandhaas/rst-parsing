( Root (span 2 22) 
  ( Nucleus (leaf 1) (rel2par span) (text _!Ministerium bestätigte Omikron-Fall in Österreich_!) )
  ( Nucleus (span 2 17) (rel2par span)
    ( Nucleus (leaf 2) (rel2par span) (text _!Die Coronavirus-Variante Omikron ist offiziell in Österreich angekommen:_!) )
    ( Satellite (span 3 17) (rel2par evidence)
      ( Nucleus (span 3 4) (rel2par span)
        ( Nucleus (leaf 3) (rel2par span) (text _!“Im Gesundheitsministerium liegen jetzt sämtliche Ergebnisse vor, die es für eine Bestätigung braucht”,_!) )
        ( Satellite (leaf 4) (rel2par attribution) (text _!hieß es aus dem Ressort._!) )
      )
      ( Satellite (span 5 17) (rel2par elaboration)
        ( Nucleus (leaf 5) (rel2par span) (text _!Bei einem Fall aus Tirol handelt es sich “mit Sicherheit” um die Variante mit der wissenschaftlichen Bezeichnung B.1.1.529._!) )
        ( Satellite (span 6 17) (rel2par e-elaboration)
          ( Nucleus (span 6 11) (rel2par span)
            ( Nucleus (span 6 9) (rel2par span)
              ( Nucleus (leaf 6) (rel2par span) (text _!Die Tiroler Behörden hatten den Verdachtsfall am Samstagabend bekanntgegeben._!) )
              ( Satellite (span 7 9) (rel2par e-elaboration)
                ( Nucleus (leaf 7) (rel2par span) (text _!Von der Infektion betroffen sei eine Person,_!) )
                ( Satellite (span 8 9) (rel2par e-elaboration)
                  ( Nucleus (leaf 8) (rel2par conjunction) (text _!die nach einer Südafrika-Reise positiv auf Covid-19 getestet wurde_!) )
                  ( Nucleus (leaf 9) (rel2par conjunction) (text _!und derzeit keine Symptome aufweise._!) )
                )
              )
            )
            ( Satellite (span 10 11) (rel2par result)
              ( Satellite (leaf 10) (rel2par attribution) (text _!Laut Elmar Rizzoli, Leiter des Tiroler Corona-Einsatzstabes,_!) )
              ( Nucleus (leaf 11) (rel2par span) (text _!wurden alle Kontaktpersonen umgehend abgesondert._!) )
            )
          )
          ( Satellite (span 12 17) (rel2par result)
            ( Nucleus (span 12 13) (rel2par span)
              ( Nucleus (leaf 12) (rel2par span) (text _!Das Land Tirol forderte alle Personen, die in den vergangenen 14 Tagen aus den Ländern Südafrika, Lesotho, Botswana, Simbabwe, Mosambik, Namibia und Eswatini zurückgekehrt sind, auf, einen PCR-Test zu machen._!) )
              ( Satellite (leaf 13) (rel2par e-elaboration) (text _!Dies sollte am fünften und zehnten Tag nach der Einreise wiederholt werden._!) )
            )
            ( Satellite (span 14 17) (rel2par result)
              ( Nucleus (leaf 14) (rel2par span) (text _!Bisher meldeten sich 31 Menschen._!) )
              ( Satellite (span 15 17) (rel2par elaboration)
                ( Nucleus (span 15 16) (rel2par span)
                  ( Nucleus (span 15 16) (rel2par span)
                    ( Nucleus (leaf 15) (rel2par list) (text _!“Neben dem genannten einen positiven Ergebnis liegt für 20 Personen bereits ein negatives Testergebnis vor,_!) )
                    ( Nucleus (leaf 16) (rel2par list) (text _!bei elf Personen sind die Testungen aktuell im Gange”,_!) )
                  )
                )
                ( Satellite (leaf 17) (rel2par attribution) (text _!hieß es._!) )
              )
            )
          )
        )
      )
    )
  )
  ( Satellite (span 18 22) (rel2par background)
    ( Nucleus (leaf 18) (rel2par span) (text _!Die Omikron-Variante von SARS-CoV-2 sorgt weltweit seit Tagen für Schlagzeilen._!) )
    ( Satellite (span 19 22) (rel2par cause)
      ( Nucleus (span 19 20) (rel2par span)
        ( Nucleus (leaf 19) (rel2par span) (text _!Die WHO arbeitet nach eigenen Angaben mit technischen Partnern zusammen,_!) )
        ( Satellite (leaf 20) (rel2par purpose) (text _!um die Auswirkungen der Variante auf die bestehenden Gegenmaßnahmen wie Impfstoffe zu bewerten._!) )
      )
      ( Satellite (span 21 22) (rel2par background)
        ( Nucleus (span 21 22) (rel2par span)
          ( Nucleus (leaf 21) (rel2par conjunction) (text _!Es sei noch unklar, ob B.1.1.259 leichter übertragbar verglichen mit anderen Covid-19-Varianten sei_!) )
          ( Nucleus (leaf 22) (rel2par conjunction) (text _!oder einen schwereren Krankheitsverlauf nach sich ziehe._!) )
        )
      )
    )
  )
)
